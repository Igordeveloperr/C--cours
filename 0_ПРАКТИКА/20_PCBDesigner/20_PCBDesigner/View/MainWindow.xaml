<Window x:Class="_20_PCBDesigner.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_20_PCBDesigner" xmlns:viewmodel="clr-namespace:_20_PCBDesigner.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <viewmodel:MainViewModel />
    </Window.DataContext>
    <Grid>
        <TextBlock Text="{Binding Click}" FontSize="30" Height="40" Width="100"></TextBlock>
        <Button Width="200" Height="40" Margin="0,90,0,0" Content="click" Command="{}"/>
    </Grid>
</Window>
